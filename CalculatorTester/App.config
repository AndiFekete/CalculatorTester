<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="remoteTestingOptions" type="System.Configuration.NameValueFileSectionHandler, 
      System, Version=1.0.5000.0, Culture=neutral, 
      PublicKeyToken=b77a5c561934e089"/>
  </configSections>
  
  <appSettings>
    <!--add key="PreferedBrowser" value="Firefox"/-->
    <add key="PreferedBrowser" value="Chrome"/>
    
    <add key="IsRemote" value="true"/>  <!--true or false-->
    
    <add key="Server" value="http://hub.browserstack.com/wd/hub"/>  <!--Uri if remote testing is selected-->
    
  </appSettings>
  
  <remoteTestingOptions>
    <add key="browserstack.user" value="[USERNAME]"/>
    <add key="browserstack.key" value="[ACCESSKEY]"/>
    <add key="browser_version" value="72.0"/>   <!-- for chrome -->
    <!--add key="browser_version" value="65.0"/-->   <!-- for firefox -->
  </remoteTestingOptions>
</configuration>